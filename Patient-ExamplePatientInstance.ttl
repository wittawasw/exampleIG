     fhir:system [ fhir:v "phone" ] ;
     fhir:value [ fhir:v "010002023" ]
  ] ) ; # 
  fhir:gender [ fhir:v "male"] ; # 
  fhir:birthDate [ fhir:v "1981-07-15"^^xsd:date] ; # 
  fhir:address ( [
     fhir:line ( [ fhir:v "123 , Random st." ] ) ;
     fhir:city [ fhir:v "City" ] ;
     fhir:postalCode [ fhir:v "12345" ] ;
     fhir:country [ fhir:v "Thailand" ]
  ] ) . # 

